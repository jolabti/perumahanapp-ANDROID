<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.perumahan"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/LayoutLogin"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.user.perumahan.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.user.perumahan.MainActivity" />
        <activity android:name="com.example.user.perumahan.ShowDataActivity" />
        <activity android:name="com.example.user.perumahan.detailActivity" />
        <activity android:name="com.example.user.perumahan.SignupActivity" />
    </application>

</manifest>